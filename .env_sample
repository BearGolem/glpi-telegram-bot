DATA_DIR=/data/

# How often bot should check the server for updates (in seconds). Default: 300 seconds
# CHECK_PERIOD=300

# Log severity. Default: INFO
# LOG_LEVEL=CRITICAL
# LOG_LEVEL=ERROR
# LOG_LEVEL=WARNING
# LOG_LEVEL=INFO
# LOG_LEVEL=DEBUG
# LOG_LEVEL=NOTSET