# API key from BotFather
TELEGRAM_TOKEN=TO_REPLACE

# URL to API
GLPI_BASE_URL=http://glpi.TO_REPLACE.example.com/apirest.php/

# Application API key. Default: ""
# GLPI_APP_API_KEY=

